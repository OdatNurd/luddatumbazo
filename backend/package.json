{
  "name": "luddatumbazo-api",
  "version": "0.0.1",
  "private": true,
  "description": "The Luddatumbazo back end; a CloudFlare Worker",
  "type": "module",
  "repository": "https://github.com/OdatNurd/luddatumbazo",
  "author": {
    "name": "OdatNurd",
    "email": "odatnurd@gmail.com",
    "url": "https://odatnurd.net"
  },
  "scripts": {
    "local:seed-db": "wrangler d1 execute luddatumbazo-db --local --file=../schema/luddatumbazo.sql",
    "run": "wrangler dev",
    "deploy": "wrangler deploy"
  },
  "devDependencies": {
    "wrangler": "^3.57.2"
  },
  "imports": {
    "#db/*": "./src/db/*.js",
    "#requests/*": "./src/requests/*.js",
    "#schema/*": "./src/schema/*.js",
    "#lib/*": "./src/lib/*.js",
    "#commit": "./commitReference.js"
  },
  "dependencies": {
    "@axel669/sanic-xml": "^0.1.2",
    "entities": "^4.5.0",
    "hono": "^3.12.6",
    "jose": "^5.2.0",
    "slug": "^8.2.3",
    "zod": "^3.22.4"
  }
}
